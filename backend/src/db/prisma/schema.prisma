generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  name      String?
  email     String   @unique
  password  String?
}

model RateLimit {
  ip          String   @id
  count       Int      @default(0)
  lastRequest BigInt   @default(0)
}
